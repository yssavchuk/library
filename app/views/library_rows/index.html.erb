<p id="notice"><%= notice %></p>

<h1>Library Rows</h1>

<table>
  <thead>
    <tr>
      <th>Book</th>
      <th>Catalog</th>
      <th>Shel</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @library_rows.each do |library_row| %>
      <tr>
        <td><%= library_row.book_id %></td>
        <td><%= library_row.catalog_id %></td>
        <td><%= library_row.shel_id %></td>
        <td><%= link_to 'Show', library_row %></td>
        <td><%= link_to 'Edit', edit_library_row_path(library_row) %></td>
        <td><%= link_to 'Destroy', library_row, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Library Row', new_library_row_path %>

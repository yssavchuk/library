<p id="notice"><%= notice %></p>

<h1>Shels</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Row</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @shels.each do |shel| %>
      <tr>
        <td><%= shel.name %></td>
        <td><%= shel.row %></td>
        <td><%= link_to 'Show', shel %></td>
        <td><%= link_to 'Edit', edit_shel_path(shel) %></td>
        <td><%= link_to 'Destroy', shel, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Shel', new_shel_path %>
